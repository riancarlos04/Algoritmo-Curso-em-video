algoritmo "vetores"
var
   n: vetor[1..4] de inteiro
   c: inteiro
inicio
    para c <- 1 ate 4 faca
           Escreva("Digite um numero para ser guardado na posiçao ", c , ":")
           Leia(n[c])
    fimpara
    Escreval(n[1])
    Escreval(n[2])
    Escreval(n[3])
    Escreval(n[4])
fimalgoritmo
